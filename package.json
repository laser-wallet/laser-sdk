{
    "name": "laser-sdk",
    "version": "0.3.0",
    "description": "sdk to interact with a Laser wallet",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "prettier:write": "npx prettier --write src/ && npx prettier --write test/",
        "test": "ts-mocha test/*.ts",
        "build": "tsc",
        "release": "yarn build && changeset publish", 
        "generate-types": "typechain --target=ethers-v5 'src/deployments/goerli/*json' --out-dir src/typechain"
    },
    "author": "Rodrigo Herrera I",
    "license": "LGPL-3.0",
    "dependencies": {
        "ethers": "^5.6.8"
    },
    "devDependencies": {
        "@changesets/cli": "^2.23.0",
        "@typechain/ethers-v5": "^10.1.0",
        "@types/chai": "^4.3.1",
        "@types/mocha": "^9.1.1",
        "chai": "^4.3.6",
        "dotenv": "^16.0.1",
        "mocha": "^10.0.0",
        "prettier": "^2.6.2",
        "ts-mocha": "^10.0.0",
        "typescript": "^4.6.4"
    }
}
